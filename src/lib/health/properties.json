{
  "birthDate": {
    "signal": "birthDate",
    "type": "date"
  },
  "age": {
    "signal": "age",
    "type": "number"
  },
  "biologicalSex": {
    "signal": "biologicalSex",
    "type": "enum",
    "localize": true,
    "options": ["male", "female", "intersex"]
  },
  "bloodType": {
    "signal": "bloodType",
    "type": "enum",
    "options": ["A+", "A-", "B+", "B-", "AB+", "AB-", "O+", "O-"]
  },
  "weight": {
    "signal": "weight",
    "type": "number",
    "unit": "kg"
  },
  "height": {
    "signal": "height",
    "type": "number",
    "unit": "cm"
  },
  "bloodPressure": {
    "signal": "bloodPressure",
    "type": "compound",
    "unit": "mmHg",
    "fields": ["systolic", "diastolic"]
  },
  "heartRate": {
    "signal": "heartRate",
    "type": "number",
    "unit": "bpm"
  },
  "temperature": {
    "signal": "temperature",
    "type": "number",
    "unit": "Â°C"
  },
  "oxygenSaturation": {
    "signal": "oxygenSaturation",
    "type": "number",
    "unit": "%"
  },
  "bloodSugar": {
    "signal": "bloodSugar",
    "type": "number",
    "unit": "mg/dL"
  }
}
